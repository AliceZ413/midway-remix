{
  "name": "midway-remix-template",
  "version": "0.1.0",
  "description": "Midway.js + Remix!",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^3.12.0",
    "@midwayjs/core": "^3.12.0",
    "@midwayjs/decorator": "^3.12.0",
    "@midwayjs/info": "^3.12.0",
    "@midwayjs/koa": "^3.12.0",
    "@midwayjs/logger": "^2.14.0",
    "@midwayjs/static-file": "^3.12.10",
    "@midwayjs/validate": "^3.12.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "antd": "^5.11.2",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.2",
    "@midwayjs/cli": "^2.0.0",
    "@midwayjs/mock": "^3.12.0",
    "@remix-run/dev": "^2.2.0",
    "@types/jest": "^29.2.0",
    "@types/koa": "^2.13.11",
    "@types/node": "14",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cross-env": "^6.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.2.2",
    "lint-staged": "^15.1.0",
    "mwtsc": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "predev": "ts-node --project ./scripts/tsconfig.node.json ./scripts/genAntdStyle.tsx",
    "prebuild": "rimraf dist build && cross-env NODE_ENV=production ts-node --project ./scripts/tsconfig.node.json ./scripts/genAntdStyle.tsx",
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "run-p dev:remix dev:midway:app",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:midway:app": "cross-env NODE_ENV=development mwtsc --watch --run @midwayjs/mock/app",
    "dev:midway:bootstrap": "cross-env NODE_ENV=development mwtsc --watch --run ./bootstrap.js",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "npm run eslint:check && npm run prettier:check",
    "eslint:check": "eslint \"{src,test,app}/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "prettier:check": "prettier --check ./**/*.{ts,js,tsx,json}",
    "prettier:write": "prettier --write ./**/*.{ts,js,tsx,json}",
    "ci": "npm run cov",
    "build": "rm -rf dist && npm run build:remix && npm run build:midway",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:midway": "cross-env NODE_ENV=production tsc"
  },
  "lint-staged": {
    "src/**/*.ts": ""
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "anonymous",
  "license": "MIT"
}